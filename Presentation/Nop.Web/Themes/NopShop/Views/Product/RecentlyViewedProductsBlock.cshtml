@model IList<ProductOverviewModel>
@using Nop.Web.Models.Catalog;
@if (Model.Count > 0)
{
    <h2>@T("Products.RecentlyViewedProducts")</h2>    
    <div class="owl-carousel owl-carousel5">
        @for (int i = 0; i < Model.Count; i++)
        {
            var product = Model[i];
            <div>
                @Html.Partial("_ProductBox", product)
            </div>
        }
    </div>    
}